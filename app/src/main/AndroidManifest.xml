<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.info.dgpaysmusicapi_v6">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.info.dgpaysmusicapi_v6.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DgPaysMusic"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DgPaysMusicApi_v5">
        <activity
            android:name="com.info.dgpaysmusicapi_v6.view.AlbumActivity"
            android:exported="true" />
        <activity
            android:name="com.info.dgpaysmusicapi_v6.view.PlaylistActivity"
            android:exported="true" />
        <activity
            android:name="com.info.dgpaysmusicapi_v6.view.PlayListArtistActivity"
            android:exported="true" />
        <activity
            android:name="com.info.dgpaysmusicapi_v6.view.TrackActivity"
            android:exported="true" />
        <activity
            android:name="com.info.dgpaysmusicapi_v6.view.ArticleActivity"
            android:exported="true" />
        <activity
            android:name="com.info.dgpaysmusicapi_v6.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>